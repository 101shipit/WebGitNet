@model TreeView

@{
    ViewBag.Title = ViewBag.RepoName;
}

@section breadcrumber
{
        <div id="breadcrumber">
            @Html.ActionLink(Html.ViewContext.RouteData.Values["controller"].ToString(), "Index") <strong>&gt;</strong> @Html.ActionLink(Html.ViewContext.RouteData.Values["repo"].ToString(), "ViewRepo", "Browse") <strong>&gt;</strong> @ViewBag.Tree <strong>&gt;</strong> @ViewBag.Path
        </div>
}

@Html.Partial("ObjectTree", Model)
