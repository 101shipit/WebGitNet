@model IList<string>

@{
    ViewBag.Title = "Repositories";
}

@section breadcrumber
{
        <div id="breadcrumber">
            @Html.ActionLink(Html.ViewContext.RouteData.Values["controller"].ToString(), "Index") <strong>&gt;</strong> @ViewBag.Title
        </div>
}

<ul>
@foreach (var repo in Model)
{
    <li>@Html.ActionLink(repo, "ViewRepo", new { repo = repo })</li>
}
</ul>

@Html.ActionLink("Create a new repo.", "Create", "Manage")
